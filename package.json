{
    "name": "@rsreborn/server",
    "version": "0.0.1",
    "description": "RS-Reborn: RuneScape server emulator.",
    "main": "./dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "npm run build && concurrently \"npm run build:watch\" \"npm start\"",
        "build": "rimraf dist && swc ./src -d dist",
        "build:watch": "swc ./src -d dist -w"
    },
    "keywords": [
        "runescape",
        "runejs",
        "typescript"
    ],
    "author": "Kikorono",
    "license": "GPL-3.0",
    "dependencies": {
        "@runejs/cache": "^0.0.2",
        "@runejs/common": "^3.0.0-beta.10",
        "adm-zip": "^0.5.9",
        "async": "^3.2.4",
        "axios": "^1.1.3",
        "bigi": "^1.4.2",
        "rxjs": "^7.5.7"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.59",
        "@swc/core": "^1.3.24",
        "@types/adm-zip": "^0.5.0",
        "@types/async": "^3.2.15",
        "@types/node": "^18.11.2",
        "chokidar": "^3.5.3",
        "concurrently": "^7.6.0",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^2.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.8.4"
    }
}
